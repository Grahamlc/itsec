<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="cn.edu.ustc.ttcn.itsec.model.ItemCon" table="s_te_itemcon" lazy="false">
		<id name="autoId" column="AutoId" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="handleId" column="HandleId" />
		
		<property name="secConLaySN" column="SecConLaySN" /><!-- 测评层面 -->
		
		<property name="itemCon" column="ItemCon" />  <!-- 控制点 -->
	</class>

</hibernate-mapping>